            FireEventNoReset("W_AddActionInputCrouch")
        elseif env(GetBehaviorRefID, SP_EF_REF_TAE_ENABLE_ADD_ACTION_INPUT_ATTACK) == TRUE then
            FireEventNoReset("W_AddActionInputAttack")
