                if _ValidateReaction(current_hkb_state, f58_local4) == TRUE then
                    return f58_local4
                end
            elseif _ValidateAction(current_hkb_state, f58_local4) == TRUE then
                return f58_local4
            end
        end
    end
    return BEH_NONE
    
end

