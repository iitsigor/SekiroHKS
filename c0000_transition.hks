function ExtractOffset(value, smallDigit, bigDidit)
    if bigDidit < smallDigit or value < 0 or smallDigit <= 0 or bigDidit <= 0 then
        return -1
    end
    return (value % 10 ^ bigDidit - value % 10 ^ (smallDigit - 1)) / 10 ^ (smallDigit - 1)
    
end

