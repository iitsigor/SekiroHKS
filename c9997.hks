            SetVariable("IndexTrunkCollapseBurst", index)
            act(SetDamageAnimType, DAMAGE_FLAG_GUARD_BREAK)
            Replanning()
            act(SuperArmorReset)
            act(SkyDeathWarp, 211)
            FaceAttacker(0)
            Fire("W_TrunkCollapseBurst")
