function GetSpDamage()
    local ret = SP_DAMAGE_NONE
    if env(GetBehaviorRefID, SP_EFFECT_REF_WIRE_ATTACK) == TRUE and (env(GetBehaviorRefID, SP_EFFECT_REF_ENABLE_WIRE_DAMAGE0) == TRUE or env(GetBehaviorRefID, SP_EFFECT_REF_ENABLE_WIRE_DAMAGE1) == TRUE or env(GetBehaviorRefID, SP_EFFECT_REF_ENABLE_WIRE_DAMAGE2) == TRUE or env(GetBehaviorRefID, SP_EFFECT_REF_ENABLE_WIRE_DAMAGE3) == TRUE or env(GetBehaviorRefID, SP_EFFECT_REF_ENABLE_WIRE_DAMAGE4) == TRUE) then
        ret = SP_DAMAGE_WIRE
    end
    if env(GetBehaviorRefID, SP_EFFECT_REF_NO_ALL_REACTION) == FALSE then
        if env(GetBehaviorID, BEH_IDENTIFIER_THROW_NEAR_REACTION) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_AI_BATTLE) == TRUE then
            ret = SP_DAMAGE_THROW_NEAR_REACTION
        end
        if env(GetBehaviorID, BEH_IDENTIFIER_THROW_FAR_REACTION) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_AI_BATTLE) == TRUE then
            ret = SP_DAMAGE_THROW_FAR_REACTION
        end
        if env(GetBehaviorID, BEH_IDENTIFIER_ROLLING) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_ENABLE_ROLLING) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_DISABLE_ROLLING) == FALSE then
            ret = SP_DAMAGE_PUSH
        end
        if env(GetBehaviorRefID, SP_EFFECT_REF_ASSASSINATION_BLOOD) == TRUE then
            ret = SP_DAMAGE_ASSASSINATION_BLOOD
        end
        if env(GetPhysicalAttribute) == DAMAGE_PHYSICAL_BURST and env(GetBehaviorRefID, SP_EFFECT_REF_NO_BURST_REACTION) == FALSE and (env(GetBehaviorRefID, SP_EFFECT_REF_BURST_ENABLE) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_BURST_ATTACK_CRETICAL) == TRUE or env(GetBehaviorRefID, SP_EFFECT_REF_BURST_ENABLE) == FALSE and env(GetBehaviorRefID, SP_EFFECT_REF_BURST_ATTACK_NONCRETICAL) == TRUE) then
            ret = SP_DAMAGE_BURST
        end
        if env(GetBehaviorRefID, SP_EFFECT_REF_ASH_BAG_ATTACK) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_NO_ASH_BAG_REACTION) == FALSE then
            ret = SP_DAMAGE_ASH_BAG
        end
        if env(GetSpecialAttribute) == DAMAGE_ELEMENT_FIRE and env(GetBehaviorRefID, SP_EFFECT_REF_FIRE_ACTION_ENABLE) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_NO_FIRE_FIAR_REACTION) == FALSE then
            ret = SP_DAMAGE_FIRE
        end
        if (env(GetSpecialAttribute) == DAMAGE_ELEMENT_LIGHTNING or env(GetBehaviorRefID, SP_EFFECT_REF_LIGHTNING_DAMAGE) == TRUE) and env(GetBehaviorRefID, SP_EFFECT_REF_LIGHTNING_DAMAGE_ENABLE) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_NO_LIGHTNING_DAMAGE) == FALSE then
            ret = SP_DAMAGE_LIGHTNING
        end
        if env(GetBehaviorID, BEH_IDENTIFIER_FIRE_FEAR) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_NO_FIRE_FIAR_REACTION) == FALSE and env(GetBehaviorRefID, SP_EFFECT_REF_FIRE_ACTION_ENABLE) == TRUE then
            ret = SP_DAMAGE_FIRE_FEAR
        end
        if env(GetBehaviorRefID, SP_EFFECT_REF_FINGER_WHISTLE) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_NO_FINGER_WHISTLE_REACTION) == FALSE then
            ret = SP_DAMAGE_FINGER_WHISTLE
        end
        if env(GetStatusActivation, STATUS_BURNING) == TRUE and IsExistAnime(ANIME_ID_FIRE_REACTION) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_NO_FIRE_REACTION) == FALSE then
            ret = SP_DAMAGE_BURNING
        end
        if env(GetStatusActivation, STATUS_SPECIAL_POISON) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_WOMAN) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_WOMAN_POISON) == TRUE then
            ret = SP_DAMAGE_POISON_REACTION
        end
        if env(GetBehaviorID, BEH_IDENTIFIER_SPIN) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_HIDE_ACTION) == TRUE then
            ret = SP_DAMAGE_HIDE_ACTION
        end
        if env(GetBehaviorID, BEH_IDENTIFIER_BACK_REALITY) == TRUE and env(GetBehaviorRefID, SP_EFFECT_REF_BACK_REALITY) == TRUE then
            ret = SP_DAMAGE_BACK_REALITY
        end
    end
    return ret
    
end

